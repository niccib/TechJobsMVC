<h1>@ViewBag.title</h1>

<h3>@ViewBag.jobs.Count Result(s)</h3>

@{
    <table class="job-listing" border="1" margin="30px auto">
        @foreach (Dictionary<string, string> job in ViewBag.jobs)
        {
            @foreach (KeyValuePair<string, string> info in job)
            {

                <tr>
                    <td>@info.Key</td>
                    <td>@info.Value</td>


                </tr>

            }
        }
    </table>
}